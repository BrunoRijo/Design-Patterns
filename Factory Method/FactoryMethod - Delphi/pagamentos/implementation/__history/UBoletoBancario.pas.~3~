unit UBoletoBancario;

interface

uses
  uIPagamento, Dialogs;

type
  BoletoBancario = class(TInterfacedObject, IPagamento)
    procedure RealizaPagamento(AValor: Double);
  end;

implementation

{ BoletoBancario }

procedure BoletoBancario.RealizaPagamento(AValor: Double);
begin
  ShowMessage('Pagamento realizado utilizado Boleto Bancário.');
end;

end.
