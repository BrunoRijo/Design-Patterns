unit UDinheiroEspecie;

interface

uses
  uIPagamento, Dialogs;

type
  DinheiroEmEspecia = class(TInterfacedObject, IPagamento)
    procedure RealizaPagamento(AValor: Double);
  end;

implementation

{ DinheiroEmEspecia }

procedure DinheiroEmEspecia.RealizaPagamento(AValor: Double);
begin
  ShowMessage('Pagamento realizado utilizado Dinheiro em espécie.');
end;

end.
